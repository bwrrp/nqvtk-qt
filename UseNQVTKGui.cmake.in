# --------------------------------------------------
# This module is provided as NQVTKGui_USE_FILE by NQVTKGuiConfig.cmake. 
# It can be INCLUDEd in a project to load the needed compiler and 
# linker settings to use NQVTKGui.

IF(NOT NQVTKGui_USE_FILE_INCLUDED)
	SET(NQVTKGui_USE_FILE_INCLUDED 1)

	# Add include directories needed to use NQVTKGui.
	INCLUDE_DIRECTORIES(${NQVTKGui_INCLUDE_DIRS})

	# Add link directories needed to use NQVTKGui.
	LINK_DIRECTORIES(${NQVTKGui_LIBRARY_DIRS})

ENDIF(NOT NQVTKGui_USE_FILE_INCLUDED)
